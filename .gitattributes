# Use Git LFS for large files only
# Large files that don't need direct processing
espscraper/data/final_product_details.jsonl filter=lfs diff=lfs merge=lfs -text

# Keep batch files as regular files for direct processing
espscraper/data/*.jsonl -filter=lfs -diff=lfs -merge=lfs -text

# Other large files that should use LFS
*.jsonl filter=lfs diff=lfs merge=lfs -text
